<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="90dp"
        android:height="90dp">
        <shape android:shape="rectangle">
            <solid
                android:color="@android:color/transparent" />
        </shape>
    </item>

    <item
        android:width="60dp"
        android:height="60dp"
        android:top="15dp"
        android:left="15dp">
        <shape android:shape="rectangle">
            <corners
                android:radius="3dp" />
            <stroke
                android:width="1dp"
                android:color="@color/contourPiece" />
            <solid android:color="@color/piece2" />
        </shape>
    </item>
    <item
        android:width="30dp"
        android:height="30dp"
        android:top="30dp"
        android:left="30dp">
        <shape android:shape="rectangle">
            <corners
                android:radius="1.5dp" />
            <solid android:color="@color/contourPiece" />
        </shape>
    </item>
</layer-list>
