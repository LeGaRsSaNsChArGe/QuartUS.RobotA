<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="22dp"
        android:height="22dp">
        <shape android:shape="rectangle">
            <solid
                android:color="@android:color/transparent" />
        </shape>
    </item>

    <item
        android:width="8dp"
        android:height="20dp"
        android:top="1dp"
        android:left="1dp">
        <shape android:shape="rectangle">
            <corners android:radius="1.5dp"/>
            <solid android:color="@color/black"/>
        </shape>
    </item>
    <item
        android:width="8dp"
        android:height="14dp"
        android:top="7dp"
        android:left="13dp">
        <shape android:shape="rectangle">
            <corners android:radius="1.5dp"/>
            <solid android:color="@color/black"/>
        </shape>
    </item>

</layer-list>
